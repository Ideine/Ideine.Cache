    <ItemGroup>
        <PackageReference Include="akavache" Version="6.0.20" />
    </ItemGroup>

    <PropertyGroup>
        <IsPackable>true</IsPackable>
        <PackageVersion>1.1.2</PackageVersion>
        <PackageId>Ideine.Cache</PackageId>
        <Title>Ideine.Cache</Title>
