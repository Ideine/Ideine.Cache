        <PackageLicense>https://github.com/Ideine/Ideine.Cache/LICENSE</PackageLicense>
        <PackageProjectUrl>https://github.com/Ideine/Ideine.Cache</PackageProjectUrl>
        <PackageTags>ideine;xmf2;cache;akavache</PackageTags>
    </PropertyGroup>
</Project>
